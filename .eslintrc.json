{
    "env": {
        "commonjs": true,
        "es2021": true,
        "es6": true,
        "node": true
    },
    "extends": ["airbnb-base","prettier","plugin:node/recommended"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier":"error",
        "no-unused-vars":"warn",
        "func-names":"off"
    }
}
